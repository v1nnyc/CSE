public class Trace {   public static void main( String[] args ) {     int[] index = {3, 2, 1, 2};     int i = index.length - 1;     while( i >= 0 )     {       bar(index[i]); i--; } }   public static void bar(int i) {     if(i == 1)     { bar1();       System.out.print("-");     }     else if(i == 2)     { bar2();       System.out.print("-");     } else { bar3();       System.out.print("-");     } }   public static void bar1() {     System.out.print( "A" ); }   public static void bar2() {     System.out.print( "B" ); }   public static void bar3() {     System.out.print( "Y" ); } }