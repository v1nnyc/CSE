import java.util.ArrayList; public class Speak { public static void main( String[] args ) { ArrayList<Animal> list = new ArrayList<Animal>();     list.add( new Cat() );     list.add( new Bird( "Tweety" ) );     list.add( new Duck( "Daisy" ) );     list.add( new Tiger( "Tony" ) );     list.add( new Lion() );     list.add( new Animal() ); for ( Animal a : list ) { System.out.println( a.getName() + " says " + a.speak() );     }   } // end main() } // end class Speak